{
  // For documentation on these settings, see: https://aka.ms/winget-settings
  "$schema": "https://aka.ms/winget-settings.schema.json",
  "experimentalFeatures": {
    "configuration": true,
    "directMSI": true,
    "experimentalARG": true,
    "experimentalCMD": false,
    "windowsFeature": true
  },
  "installBehavior": {
    "preferences": {
      "installerTypes": [
        "portable"
      ],
      "locale": [
        "en-CA",
        "fr-Ca",
        "en",
        "fr"
      ]
    }
  },
  "network": {
    "doProgressTimeoutInSeconds": 180
  },
  "source": {
    "autoUpdateIntervalInMinutes": 5
  },
  "telemetry": {
    "disable": false
  },
  "visual": {
    "progressBar": "retro"
  }
}
